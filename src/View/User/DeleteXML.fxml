<AnchorPane prefHeight="528.0" prefWidth="673.0" style="-fx-background-color: #dc80a9;" xmlns="http://javafx.com/javafx/8.0.101" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.User.DeleteController">
    <children>
