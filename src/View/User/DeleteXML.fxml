                <Font name="System Bold Italic" size="18.0" />
            </font></Label>
        <Button fx:id="yes_delete" layoutX="189.0" layoutY="385.0" mnemonicParsing="false" onAction="#deleteForever" prefHeight="33.0" prefWidth="119.0" text="Yes" />
        <Button fx:id="no_BTN" layoutX="411.0" layoutY="385.0" mnemonicParsing="false" onAction="#cancel" prefHeight="33.0" prefWidth="119.0" text="No" />
        <ImageView fitHeight="239.0" fitWidth="309.0" layoutX="232.0" layoutY="119.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../resources/img_user_remove.png" />
            </image>
        </ImageView>
    </children>
</AnchorPane>
