<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="102.0" prefWidth="249.0" xmlns="http://javafx.com/javafx/8.0.131" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MVC.DeleteController">
    <children>
        <Label fx:id="label_Message" alignment="CENTER" layoutX="60.0" prefHeight="45.0" prefWidth="129.0" text="Are you sure to delete?" />
        <Button fx:id="yes_delte" layoutX="45.0" layoutY="56.0" mnemonicParsing="false" onAction="#deleteForever" prefHeight="25.0" prefWidth="65.0" text="Yes" />
        <Button fx:id="no_BTN" layoutX="146.0" layoutY="56.0" mnemonicParsing="false" onAction="#cancel" prefHeight="25.0" prefWidth="65.0" text="No" />
    </children>
</AnchorPane>
